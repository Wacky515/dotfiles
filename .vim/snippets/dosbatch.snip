snippet backup
    set file_${1:FILE}="${2:S_FILE}"
    set link_${3:LINK}="${4:PATH}"%file_${1:FILE}%
    set trgt_${5:TRGT}=%~dp0%file_${1:FILE}%%

    if exist %link_${3:LINK}% (
        echo ^>^> Backup %link_${3:LINK}%
        rename %link_${3:LINK}% %file_${1:FILE}%".bak"
        )

